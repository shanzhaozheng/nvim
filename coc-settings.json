{
	"java.home": "/usr/local/jdk-11.0.11.jdk/Contents/Home",
	"java.configuration.maven.userSettings":"/usr/local/apache-maven-3.8.1/conf/settings.xml",
	"java.autobuild.enabled": true,
	"java.jdt.ls.vmargs": "-javaagent:/Users/szz/.config/coc/extensions/coc-java-data/lombok.jar",
	"json.format.enable": true,
	"yaml.format.enable": true,
	"coc.preferences.colorSupport": false,
  "diagnostic.displayByAle": true,
	"translator.enableHover": true,
	"explorer.sources": [{
            "name": "buffer",
            "expand": false
        },
        {
            "name": "file",
            "expand": true
        }
    ],
	"explorer.quitOnOpen": true,
	"explorer.git.enable": true,
	"explorer.git.showIgnored": true,
  "explorer.file.column.indent.indentLine": true,
  "explorer.buffer.showHiddenBuffers": false,	
	"explorer.icon.enableNerdfont": true,
	"explorer.filename.colored.enable": true,
	"explorer.keyMappings.global": {
			//"h":false,
			"il": false,
			"ic": false,
			"Ic": false,
			"Il": false,
			"II": false,
			"*": "toggleSelection",
			"<tab>": "actionMenu",
			
			"n": "collapse",
			"i": "expand",
			"u": "nodePrev",
			"e": "nodeNext",
			"U": "normal:5u",
			"E": "normal:5e",
			"o": ["wait", "expanded?", "collapse", "expand"],
			"<cr>": ["wait", "expandable?", "cd", "open"],
			"h": "open",
			"s": "open:split",
			"v": "open:vsplit",
			"t": "open:tab",
			"<bs>": ["wait", "gotoParent"],
			
			"yp": "copyFilepath",
			"yn": "copyFilename",
			"yy": "copyFile",
			"dd": "cutFile",
			"pp": "pasteFile",
			"dD": "deleteForever",
			"a": "addFile",
			"A": "addDirectory",
			"cw": "rename",
			
			"zh": "toggleHidden",
			"R": "refresh",
			
			"?": "help",
			"q": "quit",
			"<esc>": "esc",
			"X": "systemExecute",
			
			"f": "search",
			"F": "searchRecursive",
			"B": "gotoSource:file",
			"b": "gotoSource:buffer",
			"[[": "sourcePrev",
			"]]": "sourceNext",
			"[d": "diagnosticPrev",
			"]d": "diagnosticNext",
			"[c": "gitPrev",
			"]c": "gitNext",
			"<<": "gitStage",
			">>": "gitUnstage"
	},
	"languageserver": {
		"bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh"],
			"ignoredRootPaths": []
		},
  		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"],
			"initializationOptions": {
				"gocodeCompletionEnabled": true,
				"diagnosticsEnabled": true,
				"lintTool": "golint"
			}
		}
  }
}



