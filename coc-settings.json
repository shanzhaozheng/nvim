{
	"codeLens.enable": true,
	"codeLens.position": "eol",


	// 基础设置
	"coc.preferences.promptInput": true,
	"coc.preferences.messageLevel": "error",
	"coc.preferences.jumpCommand": "edit",
	"coc.preferences.rootPatterns": [ ".git", "pom.xml", "go.mod", "Cargo.toml", "Makefile" ],
	"coc.preferences.formatOnSaveFiletypes": [
		"javascript",
		"typescript",
		"html",
		"xml",
		"css",
		"json",
		"jsonc",
		"python",
		"vue",
		"svelte",
		"c",
		"cpp",
		"rust",
		"java",
		"prisma",
		"go"
	],


	"outline.previewBorderRounded": true,


	// 禁用提示源
	"coc.source.around.enable": false,
	"coc.source.buffer.enable": false,
	"coc.source.file.ignoreHidden": false,
	"coc.source.ultisnips.priority": 90,



	// 代码诊断
	"diagnostic.enable": true,
	"diagnostic.displayByAle": false,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.refreshOnInsertMode": true,
	"diagnostic.virtualText": false,
	"diagnostic.virtualTextPrefix": " ❯❯❯ ",
	"diagnostic.errorSign": "\uf467",
	"diagnostic.warningSign": "\uf071",
	"diagnostic.infoSign": "\uf129",
	"diagnostic.hintSign": "\uf864",



	// 镶嵌提示
	"inlayHint.enable": true,
	"inlayHint.refreshOnInsertMode": true,


	// CocList
	"list.previewHighlightGroup": "Statement",
	"list.smartCase": true,
	"list.nextKeymap": "<C-e>",
	"list.previousKeymap": "<C-u>",


	// 代码提示
	"suggest.noselect": false,
	"suggest.reversePumAboveCursor": true,
	"suggest.labelMaxLength": 60,
	"suggest.removeDuplicateItems": true,
	"suggest.timeout": 2000,
	"suggest.completionItemKindLabels": { // TODO 找出双class提示源
		"class": "\uf0e8",
		"color": "\ue22b",
		"constant": "\uf8fe",
		"default": "\uf29c",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"event": "\ufacd",
		"field": "\uf93d",
		"file": "\uf723",
		"folder": "\uf115",
		"function": "\u0192",
		"interface": "\uf417",
		"keyword": "\uf1de",
		"method": "\uf6a6",
		"module": "\uf40d",
		"operator": "\uf915",
		"property": "\ue624",
		"reference": "\ufa46",
		"snippet": "\ue60b",
		"struct": "\ufb44",
		"text": "\ue612",
		"typeParameter": "\uf728",
		"unit": "\uf475",
		"value": "\uf89f",
		"variable": "\ue71b"
	},

	"translator.engines": [
		"youdao"
	],


	// rust
	"rust-analyzer.enable": true,
	"rust-analyzer.lens.enable": true,
	"rust-analyzer.cargo.autoreload": true,

	// java
	"java.enabled": true,
	"java.home": "/Users/szz/.javahome/jdk-17.0.1.jdk/Contents/Home",
	"java.jdt.ls.vmargs": "-javaagent:/Users/szz/.config/coc/extensions/lombok.jar",
	"java.configuration.updateBuildConfiguration": "interactive",
	"java.configuration.maven.userSettings": "/Users/szz/.m2/settings.xml",
	"java.configuration.runtimes": [
		{
			"name": "JavaSE-1.8",
			"path": "/Library/Java/JavaVirtualMachines/jdk1.8.0_301.jdk/Contents/Home"
		},
		{
			"name": "JavaSE-11",
			"path": "/Users/szz/.javahome/jdk-11.0.13.jdk/Contents/Home",
			"default": true
		},
		{
			"name": "JavaSE-17",
			"path": "/Users/szz/.javahome/jdk-17.0.1.jdk/Contents/Home"
		} ],
	"java.referencesCodeLens.enabled": true,
	"java.implementationsCodeLens.enabled": true,
	"java.signatureHelp.enabled": true,
	"java.import.gradle.enabled": true,
	"java.import.maven.enabled": true,
	"java.autobuild.enabled": true,
	"java.maxConcurrentBuilds": 8,
	"java.completion.enabled": true,
	"java.completion.overwrite": true,
	"java.completion.guessMethodArguments": true,
	"java.format.enabled": true,
	"java.format.comments.enabled": true,
	"java.format.onType.enabled": true,
	"java.foldingRange.enabled": true,
	"java.maven.downloadSources": false,
	"java.maven.updateSnapshots": false,
	"java.saveActions.organizeImports": true,
	"java.selectionRange.enabled": true,
	"java.sources.organizeImports.starThreshold": 10,
	"java.project.referencedLibraries": [
		"lib/**/*.jar"
	],
	"java.project.resourceFilters": [
		"node_modules",
		".git",
		"doc",
		"README.md",
		"Dockerfile",
		"docker-compose.yml",
		"logs",
		"resources",
		".idea"
	],
	"java.templates.fileHeader": [
		"${user} ${date}"
	],
	"java.templates.typeComment": [
		"${user} ${date}"
	],

	// clangd
	"clangd.enabled": true,
	"clangd.path": "/usr/local/clangd_14.0.3/bin/clangd",


	// json 
	"json.enable": true,
	"json.format.enable": true,
	"json.format.keepLines": true,

	// xml
	"xml.codeLens.enabled": true,
	"xml.symbols.enabled": true,

	// ymal
	"yaml.enable": true,
	"yaml.format.enable": true,

	// html
	"html.enable": true,
	"html.format.enable": true,

	// yank
	"yank.highlight.duration": 200,
	"yank.enableCompletion": false,

	// coc-explorer
	"explorer.sources": [
		{
			"name": "buffer",
			"expand": false
		},
		{
			"name": "file",
			"expand": true
		}
	],
	"explorer.git.enable": true,
	"explorer.toggle": true,
	"explorer.git.showIgnored": true,
	"explorer.mouseMode": "singleclick",
	"explorer.file.column.indent.indentLine": true,
	"explorer.buffer.showHiddenBuffers": false,
	"explorer.icon.enableNerdfont": true,
	"explorer.filename.colored.enable": true,
	"explorer.openAction.strategy": "select",
	"explorer.quitOnOpen": false,
	"explorer.position": "floating",
	"explorer.floating.position": "right-center",
	"explorer.floating.height": 100,
	"explorer.floating.width": 50,
	"explorer.keyMappings.global": {
		"il": false,
		"ic": false,
		"Ic": false,
		"Il": false,
		"II": false,
		"x": "toggleSelection",
		"<tab>": "actionMenu",
		"n": "collapse",
		"i": "expand",
		"u": "nodePrev",
		"e": "nodeNext",
		"U": "nodePrev",
		"E": "nodeNext",
		"o": [
			"wait",
			"expanded?",
			"collapse",
			"expand"
		],
		"<cr>": [
			"wait",
			"expandable?",
			"cd",
			"open"
		],
		"h": "open",
		"s": "open:split",
		"v": "open:vsplit",
		"t": "open:tab",
		"<bs>": [
			"wait",
			"gotoParent"
		],
		"yp": "copyFilepath",
		"yn": "copyFilename",
		"yy": "copyFile",
		"dd": "cutFile",
		"pp": "pasteFile",
		"dD": "deleteForever",
		"a": "addFile",
		"A": "addDirectory",
		"cw": "rename",
		"zh": "toggleHidden",
		"R": "refresh",
		"?": "help",
		"q": "quit",
		"<esc>": "esc",
		"X": "systemExecute",
		"f": "search",
		"F": "searchRecursive",
		"B": "gotoSource:file",
		"b": "gotoSource:buffer",
		"[[": "sourcePrev",
		"]]": "sourceNext",
		"[d": "diagnosticPrev",
		"]d": "diagnosticNext",
		"[c": "gitPrev",
		"]c": "gitNext",
		"<<": "gitStage",
		">>": "gitUnstage"
	},

	//"diagnostic-languageserver.formatFiletypes": {
	//  "go": "gofmt"
	//},
	//"diagnostic-languageserver.formatters": {
	//  "gofmt": {
	//    "command": "goimports"
	//  }
	//},
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": [ "go.mod" ],
			"filetypes": [ "go" ]
		},
		"sql": {
			"command": "sql-language-server",
			"args": [
				"up",
				"--method",
				"stdio"
			],
			"filetypes": [
				"sql",
				"mysql"
			]
		},
		"asm-lsp": {
			"command": "asm-lsp",
			"filetypes": [
				"asm",
				"s",
				"S"
			]
		},
		"nginx-language-server": {
			"command": "nginx-language-server",
			"filetypes": [
				"nginx"
			]
		}
	},
	"Lua.telemetry.enable": true
}

//  vim: filetype=jsonc
